    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "2.0.1",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "acorn": "^6.1.1",
    "acorn-jsx": "^5.0.1",
    "babel-cli": "6.8.0",
    "babel-core": "^6.26.3",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
